		<script>
			// Navigation switching
			document.querySelectorAll('.nav-link[data-page]').forEach(link => {
				link.addEventListener('click', () => {
					const page = link.dataset.page;
					if (page === 'resume') return;

					document.querySelectorAll('.nav-link[data-page]').forEach(l => l.classList.remove('active'));
					link.classList.add('active');

					document.querySelectorAll('.page-section').forEach(s => s.classList.remove('active'));
					const target = document.getElementById(`page-${page}`);
					if (target) target.classList.add('active');
				});
			});

			// RACHELLM drawer
			const drawer = document.getElementById('rachellm-drawer');
			const mainContent = document.getElementById('main-content');
			const toggleBtn = document.getElementById('rachellm-toggle');
			const closeBtn = document.getElementById('drawer-close');
			const drawerToggle = document.getElementById('drawer-toggle');

			let drawerOpen = false;

			function openDrawer() {
				drawerOpen = true;
				drawer.classList.add('open');
				mainContent.classList.add('drawer-open');
			}

			function closeDrawer() {
				drawerOpen = false;
				drawer.classList.remove('open');
				mainContent.classList.remove('drawer-open');
			}

			toggleBtn.addEventListener('click', () => {
				if (drawerOpen) closeDrawer();
				else openDrawer();
			});

			closeBtn.addEventListener('click', closeDrawer);

			drawerToggle.addEventListener('click', () => {
				drawerToggle.classList.toggle('on');
			});

			// Case study data
			const projectData = [
				{
					meta: 'OPENAI X HARDWARE ¬∑ CONCEPT 2025',
					title: 'The future of AI & hardware',
					gradient: 'linear-gradient(135deg, #f4a261 0%, #e88d9e 40%, #c7a0d4 70%, #b8b0e0 100%)',
					heroTitle: 'OpenAI x Hardware',
					role: 'Lead Product Designer',
					timeline: 'August ‚Äì September 2025',
					team: '3 Designers',
					skills: 'Product Design\nProduct Strategy\nPrototyping',
					overview: 'What should OpenAI build as their first AI device?',
					body: 'We explored the intersection of AI and hardware, designing a wearable device concept called "Tomo" that integrates with ChatGPT\'s Moments feature. The goal was to create a personal AI companion that captures and remembers everyday moments, making AI feel more human and present in daily life.'
