			});

			document.getElementById('case-study-backdrop').addEventListener('click', () => {
				const overlay = document.getElementById('case-study-overlay');
				overlay.classList.remove('visible');
				setTimeout(() => { overlay.style.display = 'none'; }, 300);
			});

