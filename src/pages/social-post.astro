---
const projects = [
	{
		title: "OpenAI x Hardware",
		subtitle: "The future of AI & hardware",
		meta: "OPENAI X HARDWARE ¬∑ CONCEPT 2025",
		gradient: "linear-gradient(135deg, #f4a261 0%, #e88d9e 40%, #c7a0d4 70%, #b8b0e0 100%)",
		type: "case-study",
		size: "large",
	},
	{
		title: "PokerGPT",
		subtitle: "The world's first AI poker coach",
		meta: "POKERGPT ¬∑ SHIPPED 2023",
		gradient: "linear-gradient(160deg, #0a1628 0%, #152447 50%, #1a3060 100%)",
		type: "case-study",
		size: "large",
		lightText: true,
		mockup: "poker",
	},
	{
		title: "'Alexa,",
		subtitle: "Novel consumer AI experiences",
		meta: "AMAZON ALEXA+ ¬∑ CONTRACT 2025",
		gradient: "linear-gradient(180deg, #0a1628 0%, #0d1f3d 40%, #1a3a6a 100%)",
		type: "overview",
		size: "large",
		lightText: true,
		mockup: "alexa",
	},
	{
		title: "1Password",
		subtitle: "Bringing autofill to macOS",
		meta: "1PASSWORD ¬∑ SHIPPED 2025",
		gradient: "linear-gradient(135deg, #0a1628 0%, #111d35 50%, #1a2d52 100%)",
		type: "case-study",
		size: "large",
		lightText: true,
		mockup: "1password",
	},
	{
		title: "Figma Mobile-First",
		subtitle: "Mobile-first for Figma",
		meta: "FIGMA ¬∑ CONCEPT 2025",
		gradient: "linear-gradient(135deg, #fce4ec 0%, #f8bbd0 50%, #f48fb1 100%)",
		type: "case-study",
		size: "small",
	},
	{
		title: "Meditation Trainer",
		subtitle: "Making meditation accessible",
		meta: "SIDE PROJECT ¬∑ 2024",
		gradient: "linear-gradient(135deg, #e8eaf6 0%, #c5cae9 50%, #9fa8da 100%)",
		type: "website",
		size: "small",
	},
];

const experiences = [
	{ year: "2025", company: "Bloomberg", role: "Software Engineering Intern" },
	{ year: "2025", company: "1Password", role: "Product Design Intern" },
	{ year: "2024", company: "Royal Bank of Canada", role: "Software Engineering Intern" },
	{ year: "2023", company: "Onova", role: "Product Design + Engineering Intern" },
];

const chatSuggestions = [
	"What projects have you worked on?",
	"Tell me about your design process",
	"What was your experience at 1Password?",
];
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<title>Rachel Chen | Product Designer + Engineer</title>
		<link rel="preconnect" href="https://fonts.googleapis.com" />
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
		<link href="https://fonts.googleapis.com/css2?family=Instrument+Serif:ital@0;1&family=Inter:wght@300;400;500;600&family=JetBrains+Mono:wght@400;500&display=swap" rel="stylesheet" />
		<style>
			* { margin: 0; padding: 0; box-sizing: border-box; }

			body {
				background: #fff;
				color: #1a1a1a;
				font-family: 'Inter', -apple-system, BlinkMacSystemFont, sans-serif;
				min-height: 100vh;
				overflow-x: hidden;
			}

			/* ‚îÄ‚îÄ Navigation ‚îÄ‚îÄ */
			.nav {
				position: fixed;
				top: 0;
				left: 0;
				right: 0;
				height: 56px;
				display: flex;
				align-items: center;
				justify-content: space-between;
				padding: 0 40px;
				background: rgba(255,255,255,0.92);
				backdrop-filter: blur(12px);
				-webkit-backdrop-filter: blur(12px);
				border-bottom: 1px solid rgba(0,0,0,0.06);
				z-index: 100;
			}

			.nav-logo {
				font-family: 'JetBrains Mono', monospace;
				font-size: 13px;
				font-weight: 500;
				letter-spacing: 0.02em;
				color: #1a1a1a;
				display: flex;
				align-items: center;
				gap: 8px;
			}

			.nav-logo .dot {
				width: 10px;
				height: 10px;
				border-radius: 50%;
				background: #ff6b35;
				display: inline-block;
			}

			.nav-logo span.separator {
				color: #ccc;
				font-weight: 300;
			}

			.nav-links {
				display: flex;
				align-items: center;
				gap: 32px;
			}

			.nav-link {
				font-family: 'Inter', sans-serif;
				font-size: 13px;
				font-weight: 500;
				letter-spacing: 0.04em;
				color: #999;
				text-decoration: none;
				cursor: pointer;
				transition: color 0.2s ease;
				text-transform: uppercase;
			}

			.nav-link:hover { color: #1a1a1a; }
			.nav-link.active { color: #ff6b35; }

			.nav-link.rachellm {
				display: flex;
				align-items: center;
				gap: 6px;
			}

			.nav-link.rachellm .star {
				color: #ff6b35;
				font-size: 14px;
			}

			/* ‚îÄ‚îÄ Main Content ‚îÄ‚îÄ */
			.main {
				padding-top: 56px;
				min-height: 100vh;
				transition: margin-right 0.4s cubic-bezier(0.4, 0, 0.2, 1);
			}

			.main.drawer-open {
				margin-right: 380px;
			}

			/* ‚îÄ‚îÄ Hero Section ‚îÄ‚îÄ */
			.hero {
				padding: 80px 40px 40px;
				display: grid;
				grid-template-columns: 1fr 1fr;
				gap: 40px;
				max-width: 1200px;
				margin: 0 auto;
			}

			.hero-text h1 {
				font-family: 'Instrument Serif', Georgia, serif;
				font-size: clamp(36px, 4vw, 52px);
				font-weight: 400;
				line-height: 1.15;
				color: #1a1a1a;
				letter-spacing: -0.01em;
			}

			.hero-text h1 em {
				font-style: italic;
			}

			.timeline {
				padding-top: 12px;
			}

			.timeline-entry {
				display: grid;
				grid-template-columns: 50px 1fr 1fr;
				gap: 16px;
				padding: 8px 0;
				font-size: 14px;
				align-items: baseline;
			}

			.timeline-year {
				color: #bbb;
				font-variant-numeric: tabular-nums;
			}

			.timeline-company {
				color: #1a1a1a;
				font-weight: 500;
			}

			.timeline-role {
				color: #999;
				font-size: 13px;
			}

			/* ‚îÄ‚îÄ Project Grid ‚îÄ‚îÄ */
			.projects {
				padding: 20px 40px 80px;
				max-width: 1200px;
				margin: 0 auto;
				display: grid;
				grid-template-columns: 1fr 1fr;
				gap: 24px;
			}

			.project-card {
				position: relative;
				border-radius: 16px;
				overflow: hidden;
				cursor: pointer;
				transition: transform 0.3s ease, box-shadow 0.3s ease;
			}

			.project-card:hover {
				transform: translateY(-4px);
				box-shadow: 0 20px 60px rgba(0,0,0,0.12);
			}

			.project-card-inner {
				position: relative;
				width: 100%;
				padding-bottom: 70%;
				overflow: hidden;
			}

			.project-card-bg {
				position: absolute;
				inset: 0;
				transition: transform 0.4s ease;
			}

			.project-card:hover .project-card-bg {
				transform: scale(1.03);
			}

			.project-card-content {
				position: absolute;
				inset: 0;
				display: flex;
				flex-direction: column;
				justify-content: flex-end;
				padding: 32px;
			}

			.project-card-title {
				font-family: 'Instrument Serif', Georgia, serif;
				font-size: 28px;
				font-weight: 400;
				color: #fff;
				line-height: 1.2;
				text-shadow: 0 2px 8px rgba(0,0,0,0.15);
			}

			.project-card.dark-text .project-card-title {
				color: #1a1a1a;
				text-shadow: none;
			}

			/* Mockup illustrations */
			.project-mockup {
				position: absolute;
				top: 50%;
				right: 32px;
				transform: translateY(-50%);
			}

			.mockup-poker {
				width: 180px;
				height: 180px;
				background: rgba(255,255,255,0.12);
				border-radius: 20px;
				backdrop-filter: blur(8px);
				display: flex;
				flex-direction: column;
				padding: 20px;
				gap: 10px;
			}

			.mockup-poker .line {
				height: 8px;
				background: rgba(255,255,255,0.2);
				border-radius: 4px;
			}

			.mockup-poker .line:nth-child(1) { width: 80%; }
			.mockup-poker .line:nth-child(2) { width: 65%; background: rgba(107,79,187,0.5); }
			.mockup-poker .line:nth-child(3) { width: 90%; }
			.mockup-poker .line:nth-child(4) { width: 50%; background: rgba(107,79,187,0.5); }

			.mockup-poker .send-btn {
				margin-top: auto;
				width: 32px;
				height: 32px;
				background: rgba(107,79,187,0.6);
				border-radius: 50%;
				align-self: flex-end;
			}

			.mockup-1password {
				width: 200px;
				background: rgba(255,255,255,0.95);
				border-radius: 16px;
				padding: 24px;
				box-shadow: 0 8px 32px rgba(0,0,0,0.15);
			}

			.mockup-1password .field-label {
				font-size: 11px;
				color: #666;
				margin-bottom: 6px;
			}

			.mockup-1password .field {
				display: flex;
				align-items: center;
				gap: 8px;
				padding: 10px 14px;
				background: #f5f5f5;
				border-radius: 10px;
				margin-bottom: 14px;
			}

			.mockup-1password .field span {
				font-size: 12px;
				color: #999;
			}

			.mockup-1password .field .icon {
				width: 20px;
				height: 20px;
				background: #0066ff;
				border-radius: 50%;
				margin-left: auto;
				flex-shrink: 0;
			}

			.mockup-1password .passkey-btn {
				width: 100%;
				padding: 10px;
				background: #0066ff;
				color: white;
				border: none;
				border-radius: 10px;
				font-size: 12px;
				font-weight: 500;
				text-align: center;
			}

			.mockup-alexa {
				position: absolute;
				bottom: 40%;
				left: 32px;
				right: 32px;
			}

			.mockup-alexa .wave {
				height: 60px;
				position: relative;
				overflow: hidden;
			}

			.mockup-alexa .wave svg {
				width: 100%;
				height: 100%;
			}

			/* Hover overlay button */
			.project-card-overlay {
				position: absolute;
				inset: 0;
				display: flex;
				align-items: center;
				justify-content: center;
				opacity: 0;
				transition: opacity 0.3s ease;
			}

			.project-card:hover .project-card-overlay {
				opacity: 1;
			}

			.view-btn {
				display: inline-flex;
				align-items: center;
				gap: 8px;
				padding: 10px 20px;
				background: #ff6b35;
				color: #fff;
				border-radius: 24px;
				font-family: 'JetBrains Mono', monospace;
				font-size: 11px;
				font-weight: 500;
				letter-spacing: 0.06em;
				text-transform: uppercase;
				text-decoration: none;
				transform: translateY(8px);
				transition: transform 0.3s ease;
			}

			.project-card:hover .view-btn {
				transform: translateY(0);
			}

			.view-btn .arrow {
				font-size: 14px;
				transition: transform 0.2s ease;
			}

			.view-btn:hover .arrow {
				transform: translateX(3px);
			}

			/* Project card meta below card */
			.project-card-meta {
				display: flex;
				justify-content: space-between;
				align-items: baseline;
				padding: 12px 4px 0;
			}

			.project-card-subtitle {
				font-size: 15px;
				color: #1a1a1a;
				font-weight: 400;
			}

			.project-card-info {
				font-family: 'JetBrains Mono', monospace;
				font-size: 11px;
				color: #bbb;
				letter-spacing: 0.04em;
			}

			/* ‚îÄ‚îÄ Case Study View ‚îÄ‚îÄ */
			.case-study-overlay {
				position: fixed;
				inset: 0;
				z-index: 200;
				display: none;
				opacity: 0;
				transition: opacity 0.3s ease;
			}

			.case-study-overlay.visible {
				display: flex;
				opacity: 1;
			}

			.case-study-backdrop {
				position: absolute;
				inset: 0;
				background: rgba(0,0,0,0.3);
				backdrop-filter: blur(4px);
			}

			.case-study-panel {
				position: relative;
				width: 100%;
				height: 100%;
				background: #fff;
				display: flex;
				overflow: hidden;
				transform: translateY(20px);
				transition: transform 0.4s cubic-bezier(0.4, 0, 0.2, 1);
			}

			.case-study-overlay.visible .case-study-panel {
				transform: translateY(0);
			}

			.case-study-sidebar {
				width: 220px;
				padding: 32px 24px;
				border-right: 1px solid #eee;
				flex-shrink: 0;
			}

			.case-study-back {
				display: inline-flex;
				align-items: center;
				gap: 6px;
				font-size: 13px;
				font-weight: 500;
				color: #1a1a1a;
				text-decoration: none;
				cursor: pointer;
				margin-bottom: 32px;
				transition: color 0.2s ease;
			}

			.case-study-back:hover { color: #ff6b35; }

			.case-study-nav {
				list-style: none;
				display: flex;
				flex-direction: column;
				gap: 8px;
			}

			.case-study-nav li {
				font-size: 13px;
				color: #999;
				cursor: pointer;
				padding: 4px 0;
				transition: color 0.2s ease;
			}

			.case-study-nav li:hover { color: #1a1a1a; }
			.case-study-nav li.active { color: #1a1a1a; font-weight: 500; }

			.case-study-content {
				flex: 1;
				overflow-y: auto;
				padding: 40px 60px;
			}

			.case-study-header-meta {
				font-family: 'JetBrains Mono', monospace;
				font-size: 11px;
				color: #bbb;
				letter-spacing: 0.04em;
				margin-bottom: 12px;
			}

			.case-study-title {
				font-family: 'Instrument Serif', Georgia, serif;
				font-size: 42px;
				font-weight: 400;
				line-height: 1.15;
				margin-bottom: 32px;
			}

			.case-study-hero {
				width: 100%;
				height: 280px;
				border-radius: 16px;
				margin-bottom: 40px;
				display: flex;
				align-items: center;
				justify-content: center;
			}

			.case-study-hero-title {
				font-family: 'Instrument Serif', Georgia, serif;
				font-size: 32px;
				color: #fff;
				text-shadow: 0 2px 8px rgba(0,0,0,0.15);
			}

			.case-study-details {
				display: grid;
				grid-template-columns: repeat(4, 1fr);
				gap: 24px;
				margin-bottom: 60px;
				padding-bottom: 40px;
				border-bottom: 1px solid #eee;
			}

			.detail-label {
				font-family: 'JetBrains Mono', monospace;
				font-size: 11px;
				color: #bbb;
				letter-spacing: 0.04em;
				margin-bottom: 8px;
			}

			.detail-value {
				font-size: 14px;
				color: #1a1a1a;
				line-height: 1.5;
			}

			.case-study-section {
				margin-bottom: 48px;
			}

			.case-study-section-label {
				font-family: 'JetBrains Mono', monospace;
				font-size: 11px;
				color: #bbb;
				letter-spacing: 0.06em;
				text-transform: uppercase;
				margin-bottom: 12px;
			}

			.case-study-section h2 {
				font-family: 'Instrument Serif', Georgia, serif;
				font-size: 28px;
				font-weight: 400;
				line-height: 1.3;
				margin-bottom: 16px;
			}

			.case-study-section p {
				font-size: 15px;
				color: #666;
				line-height: 1.7;
				max-width: 620px;
			}

			/* ‚îÄ‚îÄ RACHELLM Drawer ‚îÄ‚îÄ */
			.drawer-overlay {
				position: fixed;
				top: 0;
				right: 0;
				width: 380px;
				height: 100%;
				background: #fff;
				border-left: 1px solid #eee;
				z-index: 90;
				transform: translateX(100%);
				transition: transform 0.4s cubic-bezier(0.4, 0, 0.2, 1);
				display: flex;
				flex-direction: column;
			}

			.drawer-overlay.open {
				transform: translateX(0);
			}

			.drawer-header {
				display: flex;
				align-items: center;
				justify-content: space-between;
				padding: 16px 24px;
				border-bottom: 1px solid #eee;
				height: 56px;
			}

			.drawer-title {
				font-family: 'JetBrains Mono', monospace;
				font-size: 13px;
				font-weight: 500;
				display: flex;
				align-items: center;
				gap: 8px;
			}

			.drawer-title .info-icon {
				width: 16px;
				height: 16px;
				border-radius: 50%;
				border: 1.5px solid #ccc;
				display: flex;
				align-items: center;
				justify-content: center;
				font-size: 10px;
				color: #999;
				cursor: help;
			}

			.drawer-actions {
				display: flex;
				align-items: center;
				gap: 12px;
			}

			.drawer-toggle {
				width: 36px;
				height: 20px;
				background: #ddd;
				border-radius: 10px;
				position: relative;
				cursor: pointer;
				transition: background 0.2s ease;
			}

			.drawer-toggle.on { background: #ff6b35; }

			.drawer-toggle::after {
				content: '';
				position: absolute;
				top: 2px;
				left: 2px;
				width: 16px;
				height: 16px;
				background: #fff;
				border-radius: 50%;
				transition: transform 0.2s ease;
			}

			.drawer-toggle.on::after {
				transform: translateX(16px);
			}

			.drawer-close {
				width: 28px;
				height: 28px;
				display: flex;
				align-items: center;
				justify-content: center;
				border: none;
				background: none;
				font-size: 18px;
				color: #999;
				cursor: pointer;
				border-radius: 6px;
				transition: background 0.2s ease, color 0.2s ease;
			}

			.drawer-close:hover {
				background: #f5f5f5;
				color: #1a1a1a;
			}

			.drawer-body {
				flex: 1;
				overflow-y: auto;
				padding: 32px 24px;
				display: flex;
				flex-direction: column;
			}

			.drawer-suggestions {
				margin-top: auto;
			}

			.drawer-suggestions h3 {
				font-size: 16px;
				font-weight: 500;
				color: #1a1a1a;
				margin-bottom: 16px;
			}

			.suggestion-item {
				display: flex;
				align-items: flex-start;
				gap: 10px;
				padding: 10px 0;
				cursor: pointer;
				transition: color 0.2s ease;
				color: #666;
				font-size: 14px;
				line-height: 1.4;
			}

			.suggestion-item:hover { color: #1a1a1a; }

			.suggestion-item .arrow-icon {
				color: #ff6b35;
				flex-shrink: 0;
				margin-top: 2px;
				font-size: 13px;
			}

			.drawer-input-area {
				padding: 16px 24px;
				border-top: 1px solid #eee;
			}

			.drawer-input-wrapper {
				display: flex;
				align-items: center;
				gap: 10px;
				padding: 10px 16px;
				border: 1px solid #e0e0e0;
				border-radius: 12px;
				transition: border-color 0.2s ease;
			}

			.drawer-input-wrapper:focus-within {
				border-color: #ff6b35;
			}

			.drawer-input {
				flex: 1;
				border: none;
				outline: none;
				font-family: 'Inter', sans-serif;
				font-size: 14px;
				color: #1a1a1a;
				background: transparent;
			}

			.drawer-input::placeholder { color: #bbb; }

			.drawer-send {
				width: 28px;
				height: 28px;
				display: flex;
				align-items: center;
				justify-content: center;
				background: none;
				border: none;
				color: #ccc;
				cursor: pointer;
				border-radius: 6px;
				transition: color 0.2s ease;
			}

			.drawer-send:hover { color: #ff6b35; }

			/* ‚îÄ‚îÄ Page sections for nav switching ‚îÄ‚îÄ */
			.page-section {
				display: none;
			}

			.page-section.active {
				display: block;
			}

			/* ‚îÄ‚îÄ About Page ‚îÄ‚îÄ */
			.about-section {
				max-width: 1200px;
				margin: 0 auto;
				padding: 80px 40px;
			}

			.about-section h1 {
				font-family: 'Instrument Serif', Georgia, serif;
				font-size: clamp(36px, 4vw, 52px);
				font-weight: 400;
				line-height: 1.15;
